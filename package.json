{
  "name": "miltov-language",
  "displayName": "Miltov Language",
  "description": "Syntax highlighting for Miltov language",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.50.0"
  },
  "icon": "icons/logo.png",
  "contributes": {
    "languages": [
      {
        "id": "miltov",
        "aliases": [
          "Miltov",
          "miltov"
        ],
        "extensions": [
          ".miltov"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "miltov",
        "scopeName": "source.miltov",
        "path": "./syntaxes/miltov.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "vsce": "^2.15.0"
  },
  "repository": {
  "type": "git",
  "url": "https://github.com/ave3i/miltov.language.git"
}
}